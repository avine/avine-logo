{
  "name": "avine-logo",
  "version": "1.0.0",
  "description": "My personal logo",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "less": "lessc --autoprefix='last 2 versions' --source-map src/less/index.less src/css/logo.css",
    "postless": "cleancss --source-map src/css/logo.css -o src/css/logo.min.css",
    "less-alt": "lessc --autoprefix='last 2 versions' --source-map src/less-alt/index.less src/css/logo.css",
    "postless-alt": "cleancss --source-map src/css/logo.css -o src/css/logo.min.css",
    "chokidar": "chokidar 'src/less/**/*.less' -c 'npm run less'",
    "server": "browser-sync start --server src/ --no-notify --files src/*.html src/css/*.css",
    "start": "npm run chokidar & npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avine/avine-logo.git"
  },
  "author": "avine <s.francel@avine.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/avine/avine-logo/issues"
  },
  "homepage": "https://github.com/avine/avine-logo#readme",
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "chokidar-cli": "^1.2.0",
    "less": "^2.7.2",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1"
  }
}
